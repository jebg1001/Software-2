@model IEnumerable<Software_2.Models.RegistroS>
<div class="background_edit background_total3">

<div class="p-5 col-xl-5 text-center container">
    <div>
        <select class="form-control bg-secondary"  name="" id="">
            <option value="">Ordenar por productos mas vendidos</option>
            <option value="">Ordenar por productos menos vendidos</option>
        </select>
    </div>
    <div class="p-3">
        <a href="" class="btn btn-success">Ordenar</a>
    </div>
</div>





<div class="container p-5">
        <table class="table text-center" >
        <thead class="thead-dark">
          <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Codigo</th>
            <th scope="col">Color</th>
            <th scope="col">Sexo</th>
            <th scope="col">Cantidad Vendidas</th>

          </tr>
        </thead>
        <tbody>
              @foreach (var item in Model)
    {
          <tr class="bg-secondary">
            <th scope="row">@Html.DisplayFor(modelItem => item.Nombre)</th>
            <th>@Html.DisplayFor(modelItem => item.Codigo)</th>
            <th>@Html.DisplayFor(modelItem => item.Color)</th>
            <th>@Html.DisplayFor(model => item.Sexo)</th>
            <th>@Html.DisplayFor(modelItem => item.Cantidad)</th>

          </tr>
    }
        </tbody>
      </table>     
      </div>

</div>