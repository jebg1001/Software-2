@model Software_2.Models.Registrar
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>




<div class="background_edit background_total pt-5">
 <h1 class="white" >Agrega tus Productos</h1>



<section>

  <div class="container-fluid">
    <div class="row justify-content-center" >
      <div class="col-xl-5">
          <div class="card">
            <div class="justify-content-center text-center">
              <div class="container justify-content-center text-center">
                  <div class="container col-md-10">
   
                    <form asp-controller="Home" asp-action="Registrar">
       <div class="form-group">
            <label class="control-label">Ingrese su DNI</label>
            <input asp-for="DNI" class="form-control"/>
       </div>
      <div class="form-group">
            <label class="control-label">Ingrese su nombre completo</label>
            <input asp-for="Nombre" class="form-control"/>
            </div>
       <div class="form-group">

            <label class="control-label">Ingrese su apellido completo</label>
            <input asp-for="Apellido" class="form-control"/>
          </div>
       <div class="form-group">

            <label class="control-label">Ingrese su correo</label>
            <input asp-for="Correo" class="form-control"/>
          </div>
       <div class="form-group">

            <label class="control-label">Cargo</label>
            <input asp-for="Cargo" class="form-control"/>
            </div>

       <div class="form-group">

            <label class="control-label">Ingrese su contraseña</label>
            <input asp-for="Contraseña" class="form-control"/>
          </div>
              <input class="btn btn-primary" type="submit" value="Registrar">
   </form>

   <td>@Html.DisplayNameFor(model => model.Respuesta)</td>
    <td>@Html.DisplayTextFor(model => model.Respuesta)</td>

                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
  </div>

</section>

</div>
